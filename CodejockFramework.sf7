	File.Copy(SessionVar.Expand("%DefaultSetupPath%\\%LogName_Setup%"), SessionVar.Expand("%AppFolder%\\%LogName_Setup%"), false, true, false, true, nil);
	--Folder.Delete(SessionVar.Expand("%DefaultSetupPath%"));
	SetupData.WriteToLogFile("Notice	安装路径与默认路径不相同\r\n", true);

end

